{
  "expo": {
    "name": "숫자야구",
    "slug": "rn-baseball",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/baseballIcon.png",
    "scheme": "rnbaseball",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.seongnam.rnbaseball",
      "supportsTablet": false,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/baseballIcon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8a4ee16d-3b31-463e-ad20-b6f27ddebc0f"
      }
    },
    "owner": "seongnam"
  }
}
